<div class="row">
  <!-- left column -->
  <div class="col-md-6">
    <!-- general form elements -->
    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="box-title">Perfil clinico</h3>
      </div>
      <!-- /.box-header -->
        <div class="box-body">
          <p id="notice"><%= notice %></p>
          <div class="form-group">
            <strong>Nombre/s:</strong>
  <%= @clinic_profile.first_name %>
          </div>
          <div class="form-group">
            <strong>Apellidos:</strong>
  <%= @clinic_profile.last_name %>
          </div>
          <div class="form-group">
          <strong>Fecha de nacimiento:</strong>
  <%= @clinic_profile.birthday %>
          </div>
          <div class="form-group">
              <strong>Parentesco:</strong>
  <%= @clinic_profile.relationship %>
          </div>
          <div class="form-group">
            <strong>Genero:</strong>
  <%= @clinic_profile.genre %>
          </div>
          <div class="form-group">
            <strong>Tipo de sangre:</strong>
  <%= @clinic_profile.blood %>
          </div>
          <div class="form-group">
            <strong>Altura:</strong>
  <%= @clinic_profile.height %>
          </div>
          <div class="form-group">
            <strong>Peso:</strong>
  <%= @clinic_profile.weight %>
          </div>
        </div>
        <!-- /.box-body -->
    </div>
    <!-- /.box -->
  </div>
  <!--/.col (left) -->
</div>
