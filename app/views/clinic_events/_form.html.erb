<div class="row">
  <!-- left column -->
  <div class="col-md-6">
    <!-- general form elements -->
    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="box-title">Nuevo evento</h3>
      </div>
      <!-- /.box-header -->
      <!-- form start -->
      <%= form_for(@clinic_event) do |f| %>
        <div class="box-body">
          <% if @clinic_event.errors.any? %>
            <div id="error_explanation">
              <h2><%= pluralize(@clinic_event.errors.count, "error") %> prohibited this clinic_event from being saved:</h2>

              <ul>
              <% @clinic_event.errors.full_messages.each do |message| %>
                <li><%= message %></li>
              <% end %>
              </ul>
            </div>
          <% end %>
          <div class="form-group">
            <label>Diagnostico</label>
            <%= f.text_field :diagnostic, class: "form-control"  %>
          </div>
          <div class="form-group">
            <label>Prediagnostico</label>
            <%= f.text_field :prediagnostic, class: "form-control"  %>
          </div>
          <div class="form-group">
            <label>Sintomas</label>
            <%= f.text_field :symptoms, class: "form-control"  %>
          </div>
          <div class="form-group">
            <label>Fecha inicio</label>
            <%= f.date_select :date_start, class: "form-control"  %>
          </div>
          <div class="form-group">
            <label>Fecha termino</label>
            <%= f.date_select :date_finish, class: "form-control"  %>
          </div>
          <div class="form-group">
            <label>Prescripcion</label>
            <%= f.text_field :prescription, class: "form-control"  %>
          </div>
        </div>
        <!-- /.box-body -->

        <div class="box-footer">
          <%= f.submit%>
        </div>
      <% end %>
    </div>
    <!-- /.box -->
  </div>
  <!--/.col (left) -->
</div>